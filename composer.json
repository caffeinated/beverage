{
    "name": "caffeinated/beverage",
    "description": "Laravel 5 support package",
    "keywords": ["laravel", "caffeinated", "beverage", "repositories", "structure"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        },
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "danielstjules/stringy": "~1.8",
        "illuminate/support": "5.1.*",
        "illuminate/filesystem": "5.1.*",
        "illuminate/config": "5.1.*",
        "illuminate/console": "5.1.*",
        "symfony/filesystem": "~2.7",
        "webmozart/path-util": "~1.0",
        "anahkiasen/underscore-php": "2.0.*",
        "vlucas/phpdotenv": "1.0.*"
    },
    "require-dev": {
        "caffeinated/dev": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Caffeinated\\Beverage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinated\\Tests\\Beverage\\": "tests/"
        }
    }
}
